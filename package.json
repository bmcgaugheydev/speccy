{
  "name": "speccy",
  "version": "0.11.0",
  "description": "An OpenAPI v3.0 development workflow assistant",
  "homepage": "https://speccy.io/",
  "packageManager": "yarn@3.2.2",
  "bin": "./speccy.js",
  "scripts": {
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc.js --write \"**/*.{js,jsx}\"",
    "lint": "./node_modules/.bin/eslint test --ext .jsx --ext .js --ext --.json"
  },
  "preferGlobal": true,
  "repository": {
    "url": "https://github.com/wework/speccy.git",
    "type": "git"
  },
  "author": "Phil Sturgeon <phil.sturgeon@wework.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wework/speccy/issues"
  },
  "keywords": [
    "openapi",
    "openapi3",
    "swagger",
    "documentation",
    "validator",
    "validation",
    "resolver",
    "lint",
    "linter"
  ],
  "dependencies": {
    "@openapi-contrib/json-schema-to-openapi-schema": "^2.2.5",
    "commander": "^11.0.0",
    "nconf": "^0.12.0",
    "nconf-yaml": "^1.0.2",
    "node-fetch": "^3.3.1",
    "node-readfiles": "^0.3.1",
    "oas-linter": "^3.2.2",
    "oas-resolver": "^2.5.6",
    "oas-validator": "^5.0.8",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.2",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "mock-stdin": "^1.0.0",
    "nock": "^13.3.1",
    "prettier": "^2.8.8"
  }
}
